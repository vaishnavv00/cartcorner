{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URL": "@mongodb_url",
    "JWT_SECRET": "@jwt_secret",
    "CLOUD_NAME": "@cloud_name",
    "API_KEY": "@api_key",
    "SECRET_KEY": "@secret_key",
    "MAIL_ID": "@mail_id",
    "MP": "@mp",
    "RAZORPAY_KEY_ID": "@razorpay_key_id",
    "RAZORPAY_SECRET": "@razorpay_secret"
  }
}
